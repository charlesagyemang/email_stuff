<!--[event,current_organisation.build] -->

<%= form_for(@event) do |f| %>

        <%= f.label :event_name ,"Event Name" %>
        <%= f.text_field :event_name  %>
        <br>
        <br>
        <%= f.label :event_type , "Event type" %>
        <%= f.text_field :event_type  %>
         <br>
         <br>
         <%= f.label :event_date, "Event Date" %>
         <%= f.datetime_field :event_date  %>

         <%= f.submit "Submit" %>

<% end %>